{
  "name": "parrotsour-components",
  "version": "4.2.0",
  "private": false,
  "license": "MIT",
  "homepage": "https://www.parrotsour.com",
  "main": "dist/index.tsx",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://www.github.com/jemccarthy13/parrotsour"
  },
  "dependencies": {
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "compromise": "^14.11.0",
    "compromise-sentences": "^0.3.0",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.21.2",
    "react-router-dom": "^6.21.2",
    "react-speech-recognition": "^3.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/react-dom": "^18.2.18",
    "@types/react-router": "^5.1.20",
    "@types/react-speech-recognition": "^3.9.5",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cross-env": "^7.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prototype-pollution-security-rules": "^1.0.6",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-regexp": "^2.2.0",
    "hasha": "^6.0.0",
    "history": "^5.3.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "pretty-format": "^29.7.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-standalone": "json --in-place -f package.json -e 'this.homepage=\".\"' && react-scripts build",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --extensions \".ts,.js,.tsx,.jsx\" --out-dir dist --no-copy-ignored",
    "pub": "npm run compile && npm publish",
    "clean": "rimraf dist && rimraf build && rimraf coverage",
    "clean:build": "npm run clean && npm install && npm run build",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fixdr": "eslint src/**/*.{ts,tsx} --fix-dry-run",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "test": "react-scripts test --watchAll=false --maxWorkers=75%",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "react-scripts test --watchAll --maxWorkers=75%",
    "ci-coverage": "npm run ci-test -- --coverage",
    "ci-test": "npm run test -- --silent",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/jsdom": "16.5.3"
  },
  "jest": {
    "snapshotSerializers": [
      "./src/testutils/canvas-serializer.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/components/procedural/ai.js",
      "./src/components/procedural/aiprocess.js",
      "./src/ai/*"
    ]
  }
}
