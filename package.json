{
  "name": "parrotsour-components",
  "version": "4.1.2",
  "private": false,
  "license": "MIT",
  "homepage": "http://www.parrotsour.com",
  "main": "dist/index.tsx",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://www.github.com/jemccarthy13/parrotsour"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "compromise": "^13.11.4",
    "compromise-sentences": "^0.3.0",
    "hasha": "^5.2.2",
    "notistack": "^2.0.8",
    "pretty-format": "^29.3.1",
    "react": "^18.2.0",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.1",
    "react-speech-recognition": "^3.10.0",
    "styled-components": "^5.3.6",
    "tss-react": "^4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.5",
    "@types/react-dom": "^18.0.10",
    "@types/react-router": "^5.1.20",
    "@types/react-speech-recognition": "^3.9.0",
    "@types/styled-components": "^5.1.26",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "canvas": "^2.11.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-prototype-pollution-security-rules": "^1.0.6",
    "eslint-plugin-react": "^7.32.0",
    "history": "^5.3.0",
    "jsdom-canvas-2": "^11.6.3-alpha.0",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-standalone": "json --in-place -f package.json -e 'this.homepage=\".\"' && react-scripts build",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --extensions \".ts,.js,.tsx,.jsx\" --out-dir dist --no-copy-ignored",
    "pub": "npm run compile && npm publish",
    "clean": "rimraf dist && rimraf build && rimraf coverage",
    "clean-build": "npm run clean && npm install && npm run build",
    "eject": "react-scripts eject",
    "clint": "eslint src/**/*.{ts,tsx}",
    "clint-fixdr": "eslint src/**/*.{ts,tsx} --fix-dry-run",
    "clint-fix": "eslint src/**/*.{ts,tsx} --fix",
    "coverage": "npm run test -- --coverage --detectOpenHandles",
    "test": "react-scripts test --watchAll",
    "ci-coverage": "npm run ci-test -- --coverage",
    "ci-test": "react-scripts test --maxWorkers=75% --watchAll=false --silent"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/jsdom": "16.5.3"
  },
  "jest": {
    "snapshotSerializers": [
      "./src/testutils/canvas-serializer.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/pscomponents/procedural/ai.js",
      "./src/pscomponents/procedural/aiprocess.js",
      "./src/ai/*"
    ]
  }
}
